{
    "name": "tecnodes/encore",
    "type": "project",
    "description": "The Encore Application.",
    "keywords": [
        "cms",
        "encoredcms",
        "multilingual",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "astrotomic/laravel-translatable": "^11.10",
        "cartalyst/sentinel": "^7.0",
        "composer/installers": "^2.1",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.2",
        "illuminate/support": "^10.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.5",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.3",
        "mcamara/laravel-localization": "^1.8",
        "spatie/laravel-responsecache": "^7.4",
        "symfony/yaml": "^6.0",
        "tecnodesignc/laravel-menus": "^9.0",
        "tecnodesignc/presenter": "^0.2.6",
        "tecnodesignc/stylist": "^2.1",
        "tightenco/ziggy": "^1.4",
        "typicms/nestablecollection": "^3.0",
        "venturecraft/revisionable": "^1.39",
        "yajra/laravel-datatables-oracle": "^10.3.1",
        "tecnodesignc/adminlte-theme": "^2.0",
        "tecnodesignc/core-module": "^2.1",
        "tecnodesignc/dashboard-module": "^2.1",
        "tecnodesignc/media-module": "^2.1",
        "tecnodesignc/menu-module": "^2.1",
        "tecnodesignc/page-module": "^2.1",
        "tecnodesignc/setting-module": "^2.1",
        "tecnodesignc/tag-module": "^2.1",
        "tecnodesignc/translation-module": "^2.1",
        "tecnodesignc/user-module": "^2.1",
        "tecnodesignc/workshop-module": "^2.1",
        "tecnodesignc/encore-theme": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tecnodesignc/installers.git"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "^2.0": "2.0.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "composer/installers": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
